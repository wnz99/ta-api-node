{
  "name": "ta-api-node",
  "version": "0.1.3",
  "description": "",
  "homepage": "https://github.com/wnz99/ta-api-node",
  "repository": {
    "type": "git",
    "url": "https://github.com/wnz99/ta-api-node.git"
  },
  "main": "dist/ta.js",
  "standard": {
    "ignore": [
      "/examples",
      "/dist"
    ]
  },
  "scripts": {
    "build": "npm run build:web:browserify; npm run build:web:minify; npm run build:web:hash",
    "build:web:browserify": "rm -rf ./dist/*.js; browserify ./src/ta.js --standalone TA -o ./dist/ta.js",
    "build:web:minify": "node-minify --compressor uglify-es --input './dist/ta.js' --output './dist/ta.min.js'",
    "build:web:hash": "hash-filename ./dist/ta*.js; rm -rf ./dist/{ta.js,ta.min.js}",
    "build:web:run": "browserify --debug ./src/ta.js --standalone TA | browser-run --port 2222",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "release": "git fetch && np"
  },
  "author": "David Fava <david@tokenanalyst.io>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "browserify": "^16.5.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "hash-filename": "^1.0.4",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "node-minify": "^3.6.0",
    "prettier": "1.18.2",
    "prettier-eslint": "^9.0.0"
  }
}
